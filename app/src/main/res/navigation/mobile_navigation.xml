<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_imaging">

    <fragment
        android:id="@+id/navigation_imaging"
        android:name="com.uf.automoth.ui.imaging.ImagingFragment"
        android:label="@string/title_imaging"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.uf.automoth.ui.data.DataFragment"
        android:label="@string/title_data"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_other"
        android:name="com.uf.automoth.ui.info.InfoFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_notifications" />
</navigation>