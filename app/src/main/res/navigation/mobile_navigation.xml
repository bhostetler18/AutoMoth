<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_imaging">

    <fragment
        android:id="@+id/navigation_imaging"
        android:name="com.uf.biolens.ui.imaging.ImagingFragment"
        android:label="@string/title_imaging"
        tools:layout="@layout/fragment_imaging" />

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.uf.biolens.ui.sessions.SessionsFragment"
        android:label="@string/title_sessions"
        tools:layout="@layout/fragment_sessions" />

    <fragment
        android:id="@+id/navigation_info"
        android:name="com.uf.biolens.ui.info.InfoFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.uf.biolens.ui.BioLensPreferenceFragment"
        android:label="@string/settings" />
</navigation>